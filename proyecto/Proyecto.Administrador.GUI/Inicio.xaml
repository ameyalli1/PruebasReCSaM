<Window x:Class="Proyecto.Administrador.WPF.GUI.Inicio"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Proyecto.Administrador.GUI"
        mc:Ignorable="d"
        Title="ReCSaM Inicio" Height="350" Width="515">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="56*"/>
            <RowDefinition Height="113*"/>
            <RowDefinition Height="150*"/>
        </Grid.RowDefinitions>
        <ProgressBar Height="20" Width="300" x:Name="BarraProgreso" Grid.Column="0" Grid.Row="1"  ></ProgressBar>
        <TextBox Grid.Column="0" Grid.Row="3" x:Name="txtLeyenda" Height="20" Width="250" ></TextBox>
    </Grid>
</Window>
