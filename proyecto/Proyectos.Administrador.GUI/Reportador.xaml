<Window x:Class="Proyectos.Administrador.GUI.Reportador"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Proyectos.Administrador.GUI"
        xmlns:rv="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"
        mc:Ignorable="d"
        Title="Reporte" Height="600" Width="750" Background="#FFCDABE6">
   
    <Grid>
        <WindowsFormsHost Background="#FFCDABE6">
            <rv:ReportViewer x:Name="Contenedor"/>
        </WindowsFormsHost>

    </Grid>
</Window>
