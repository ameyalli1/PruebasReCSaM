<Window x:Class="Proyecto.GUI.MenuPrincipal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Proyecto.GUI"
        mc:Ignorable="d"
        Title="Sistema cambiale el nombre" Height="350" Width="515">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="114*"/>
            <RowDefinition Height="115*"/>
            <RowDefinition Height="90"/>
        </Grid.RowDefinitions>
        <Label Grid.Row="0" Grid.Column="0" HorizontalAlignment="Center" FontSize="24" FontWeight="Bold" FontStyle="Italic" Margin="60,25,61,25" Width="386">Sistema falta el nombre agregalo</Label>
        <Label Grid.Column="0" Grid.Row="1" HorizontalAlignment="Center" Margin="204,0" Width="99">Logo del sistema</Label>
        <Button Grid.Row="2" BorderBrush="Black" Background="{x:Null}" Margin="214,29,213,29" x:Name="btnContinuar" Click="btnContinuar_Click">
            <StackPanel>
                <Label>Continuar</Label>
            </StackPanel>
        </Button>
    </Grid>
</Window>
